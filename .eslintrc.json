{
  "plugins": [
    "react"
  ],
  "extends": [
    "airbnb",
    "plugin:react/recommended"
  ],
  "parser": "babel-eslint",
  "rules": {
    "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }],
    "react/prop-types": 1,
    "react/no-array-index-key": 0,
    "react/require-default-props": 0,
    "react/jsx-tag-spacing": 0,
    "import/no-named-default": 0,
    "no-shadow": 0,
    "consistent-return": 1
  }
}
